{
  "name": "Avocado",
  "version": "1.0.1",
  "author": {
    "name": "Justyna Rachowicz",
    "email": "mrowa44+avocado@gmail.com"
  },
  "description": "Avocado, fine pomodoro timer for macOS",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mrowa44/avocado"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "electron-context-menu": "^0.9.1",
    "electron-store": "^1.1.0",
    "electron-updater": "^2.16.1",
    "lodash.remove": "^4.7.0",
    "moment": "^2.18.1",
    "mousetrap": "^1.6.1",
    "prop-types": "^15.5.10",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "electron": "^1.7.10",
    "electron-builder": "^19.45.5",
    "electron-is-dev": "^0.3.0",
    "electron-reloader": "^0.1.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "pre-push": "^0.1.1",
    "react-scripts": "1.0.17",
    "sass-lint": "^1.10.2",
    "wait-on": "^2.0.2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "pre-push": [
    "lint",
    "sass-lint"
  ],
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "build/icon.icns",
      "publish": "github"
    }
  },
  "scripts": {
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "eject": "react-scripts eject",
    "electron-pack": "build --em.main=build/electron.js",
    "lint": "eslint 'src/**/*.js'",
    "preelectron-pack": "yarn build",
    "postinstall": "electron-builder install-app-deps",
    "sass-lint": "sass-lint -c .sass-lint.yml 'src/*.scss' -v",
    "start": "BROWSER=none npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  }
}
